{
  "name": "@patchwallet/patch-sdk",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "description": "SDK for Patch Wallet",
  "version": "0.0.5",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "default": "./dist/src/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "scripts": {
    "test": "bun test",
    "build": "rm -rf ./dist && rm ./tsconfig.tsbuildinfo && bun build ./src/index.ts --outdir=./dist/src --target=node --format=esm --minify --sourcemap=external && bun tsc --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaymagicXYZ/patch-sdk.git"
  },
  "keywords": [
    "patchwallet"
  ],
  "author": "mikelxc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaymagicXYZ/patch-sdk/issues"
  },
  "homepage": "https://github.com/PaymagicXYZ/patch-sdk#readme"
}